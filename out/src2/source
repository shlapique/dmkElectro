    InitFormPerehvat();
    function InitFormPerehvat() {
        $('#form0').submit = function (e) {
            e.preventDefault();
            SubmitPageForm();
            return SubmitPageForm();
        };
        $('#form0').keypress(function (event) {
            if (event.keyCode == '13') {
                SubmitPageForm();
                event.preventDefault();
            }
        });
    }
    function SubmitPageForm()
    {
        var c = TryParseInt($("#PageSelector").val(), null);
        if (c!=null &&c > 0 && c <= TryParseInt($("#bmkpagetotalnum").text(),null)) {
            var $form = $('#form0');
            $.ajax({
                type: "POST",
                url: $form.attr('action'),
                data: $form.serialize(),
                error: function (xhr, status, error) {
                    alert("При обработке формы запроса произошла ошибка!");
                },
                beforeSend: function () {
                    GetScroll();
                    $("#LoadingElem").show();
                },
                success: function (data) {
                    $("#LoadingElem").hide();
                    SetPageData(data);
                    //SetSearchData(data);
                }
            });
            return true;
        }
        else
        {
            alert("Введите  от 1 до " + TryParseInt($("#bmkpagetotalnum").text(), null));
            return false;
        }
    }
    function TryParseInt(str, defaultValue) {
        var retValue = defaultValue;
        if (str !== null) {
            if (str.length > 0) {
                if (!isNaN(str)) {
                    retValue = parseInt(str);
                }
            }
        }
        return retValue;
    }

