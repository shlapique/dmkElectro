    <script type="text/javascript">
        var mashtb;//масштаб
        var lock = false;
        var IgnoreScrollTop = false;
        var TopCont = 0;
        var scroll;//для хранения скролла странице по x и y
        //сохранить параметры страницы
        function GetScroll() {
            mashtb = $("#slider2").slider("value");
            scroll = getPageScroll();
        }
        //распарсить данные страницы
        function SetPageData(data) {
            var div = document.getElementById('PageSwitcher');
            var r = data.match(/<div id="BookPagesAreaInner">([\s\S]*)<\/div>/gi);
            document.getElementById('BookPagesArea').innerHTML = r;
            data = data.replace(/<div id="BookPagesAreaInner">([\s\S]*)<\/div>/gi, ' ');
            data.replace(/<img /gi, '<img onload ');
            div.innerHTML = data;
            InitFormPerehvat();
        }
        //настройка параметров страницы после загрузки: масштаб и отступы
        function ImgReverse(id, pageid, flag) {
            //alert(mashtb);
            //$("#pgimg").width("" + mashtb + "%");
            $("#pgimg").each(function (i, obj) { $(this).width("" + Math.round(obj.naturalWidth * $("#slider2").slider("value") / 100) + "px"); });
            if (IgnoreScrollTop == false) {
                SetPageScrollY(scroll[0]);
            } else {
                SetPageScrollY(TopCont);
            }
            SetPageScrollX(scroll[1]);
            document.getElementById(id).scrollLeft = document.getElementById('memory').getAttribute('leftpp');
            lock = false;
            if (flag == 'True') {
                SetScrollThumb(pageid);
            }
            IgnoreScrollTop == false;
            SetPageHighlight();
        }
        //инициировать скроллер и масштаб
        function SetStart() {
            var d = document.getElementById('slider2');
            if (d != null) {
                InitScroller();
                if (document.addEventListener) { //code for Moz
                    document.addEventListener("keydown", keyCapt, false);
                } else {
                    document.attachEvent("onkeydown", keyCapt); //code for IE
                }

            }

        }
        //перейти к странице с pageid
        function SetScrollThumb(pageid) {
            $('.BookThumbContainer .thumb a').removeClass("ChekedPage");//TODO: СДЕЛАТЬ НОРМАЛЬНО
            $(pageid).addClass("ChekedPage");
            $('.BookThumbContainer').scrollTop(
                $(pageid).offset().top
                - $('#PageN1').offset().top
                );
        }
        //перехват клавиш
        function keyCapt(e) {

            if (typeof window.event != "undefined") {
                e = window.event;//code for IE
            }
            switch (e.keyCode) {
                case 33:
                    if (getPageScroll()[0] == 0) {
                        TopCont = $(document).height() - $(window).height();
                        IgnoreScrollTop = true;
                        document.getElementById('PPrev').click();
                    }
                    break;
                case 34:
                    PNext
                    if ((getPageScroll()[0] == $(document).height() - $(window).height())) {
                        TopCont = 0;
                        IgnoreScrollTop = true;
                        document.getElementById('PNext').click();
                    }
                    break;
                case 79:
                    SetPageScrollY(200);
                    break;
            }
        }
        //получить данные о скролле
        function getPageScroll() {
            var xScroll, yScroll;
            if (self.pageYOffset) {
                yScroll = self.pageYOffset;
                xScroll = self.pageXOffset;
            } else if (document.documentElement && document.documentElement.scrollTop) {
                yScroll = document.documentElement.scrollTop;
                xScroll = document.documentElement.scrollLeft;
            } else if (document.body) {// all other Explorers
                yScroll = document.body.scrollTop;
                xScroll = document.body.scrollLeft;
            }
            return new Array(yScroll, xScroll)
        }
        //установить скролл по Y
        function SetPageScrollY(y) {
            //alert("SetPageScrollY " + y);
            //if (self.pageYOffset) {
            //    self.pageYOffset=y;
            //} else
            if (document.documentElement && document.documentElement.scrollTop) {
                document.documentElement.scrollTop = y;
            } else if (document.body) {// all other Explorers
                document.body.scrollTop = y;
            }
        }
        //установить скролл по X
        function SetPageScrollX(x) {
            //alert("SetPageScrollX "+x);
            //if (self.pageYOffset) {
            //    self.pageXOffset = x;
            //} else
            if (document.documentElement && document.documentElement.scrollTop) {
                document.documentElement.scrollLeft = x;
            } else if (document.body) {// all other Explorers
                document.body.scrollLeft = x;
            }
        }
        //инициация скролла
        function InitScroller() {
            $("#slider2").slider();
            //$("#slider2").slider( "disable" );
            $("#slider2 .ui-slider-handle").unbind('keydown');
            $("#slider2").slider("option", "step", 5);
            $("#slider2").slider("option", "min", 40);
            $("#slider2").slider("option", "max", 200);

            $("#slider2").on("slide", function (event, ui) {
                $("#info").val(ui.value);
                //вычисление оригинального размера
                $("#pgimg").each(function (i, obj) {
                    //alert(obj.naturalWidth);
                    $(this).width("" + Math.round(obj.naturalWidth * ui.value / 100) + "px");
                });
                ResizeHighlight($("#slider2").slider("value"), ui.value);
            });
            $("#slider2").slider("value", 100);
            $("#info").val(100);
            $("#info").not(':focus');
        }
        
        function EditBmk(id, MO_ID) {
            $.ajax({
                type: "GET",
                url: '/ProtectedView/Bookmark/Edit/' + id + '?MO_ID=' + MO_ID,
                    //data: "{bookid:'" + bid + "',page:''}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "html",
                    success: function (data) {
                        SetPopUp('Редактировать закладку', data);
                        $('#TagListEditor').tagsInput({
                            'height': '100px',
                            'width': '100%',
                            'interactive': true,
                            'defaultText': 'Добавьте теги',
                            'onAddTag': function (tag) {
                                $('#TagList').val($('#TagList').val() + tag + ',');
                            },//SetTags('TagListEditor','TagList'),
                            'onRemoveTag': function (tag) {
                                $('#TagList').val($('#TagList').val().replace(tag + ',', ''));
                            },
                            //'onChange': SetTags('TagListEditor', 'TagList'),
                            'removeWithBackspace': true,
                            'placeholderColor': '#666666'
                        });
                    },
                    error: function (data) {
                        alert('ВНИМАНИЕ: Возникла временная перегрузка сервера. Посчитайте до трех и повторите попытку.');
                    }
                })
        }

            function DellBmk(id, MO_ID) {
                $.ajax({
                    type: "GET",
                    url: '/ProtectedView/Bookmark/Delete/' + id + '?MO_ID=' + MO_ID,
                    //data: "{bookid:'" + bid + "',page:''}",
                    contentType: "application/json; charset=utf-8",
                    dataType: "html",
                    success: function (data) {
                        RefreshBmk(MO_ID);
                    },
                    error: function (data) {
                        alert('ВНИМАНИЕ: Возникла временная перегрузка сервера. Посчитайте до трех и повторите попытку.');
                    }
                })
            }

            function RefreshBmk(r) {
                //alert('111DELL');
                if (document.getElementById('bmkblock' + r) == null) {
                    var bid = document.getElementById('bmkbookid').innerText;
                    $.ajax({
                        url: '/ProtectedView/Bookmark?bookid=' + bid,
            type: "GET",
            dataType: "html",
            cache: false,
            success: function (data) {
                $("#BookmarkList").html(data);
            },
            error: function () {
                alert('ВНИМАНИЕ: Возникла временная перегрузка сервера. Посчитайте до трех и повторите попытку.');
            }
        });
    }
    else {
        $.ajax({
            url: '/ProtectedView/Bookmark/FullList?MO_ID=' + r,
            type: "GET",
            dataType: "html",
            cache: false,
            success: function (data) {
                $("#bmkblock" + r).html(data);
            },
            error: function () {
                alert('ВНИМАНИЕ: Возникла временная перегрузка сервера. Посчитайте до трех и повторите попытку.');
            }
        });
    }
}
function AddBmk() {
    //alert("!!!!!!!");
    //найти pagenum
    var pnum = $('#bmkpagenum').text();
    var MOID = $('#bmkbookid').text();
    //alert('/ProtectedView/Bookmark/Create/' + MOID + '?page=' + pnum);
    $.ajax({
        type: "GET",
        url: '/ProtectedView/Bookmark/Create/' + MOID + '?page=' + pnum,
        //data: "{bookid:'" + bid + "',page:''}",
        contentType: "application/json; charset=utf-8",
        dataType: "html",
        success: function (data) {
            SetPopUp('Создать закладку', data);
            $('#TagListEditor').tagsInput({
                'height': '100px',
                'width': '100%',
                'interactive': true,
                'defaultText': 'Добавьте ключевые слова',
                'onAddTag': function (tag) {
                    //alert(tag);
                    $('#TagList').val($('#TagList').val() + tag + ',');
                },//SetTags('TagListEditor','TagList'),
                'onRemoveTag': function (tag) {
                    //alert(tag);
                    $('#TagList').val($('#TagList').val().replace(tag + ',', ''));
                },
                //'onChange': SetTags('TagListEditor', 'TagList'),
                'removeWithBackspace': true,
                'placeholderColor': '#666666'
            });
        },
        error: function (data) {
            alert('ВНИМАНИЕ: Возникла временная перегрузка сервера. Посчитайте до трех и повторите попытку.');
        }
    })
}
