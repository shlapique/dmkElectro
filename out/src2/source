        <div id="SearchLoadingElem">
    <img src="/Content/loading17.gif" />Идет поиск по документу</div>
<div id="SeachPanel" class="SeachPanel hiddenstrip">
    <div style="width: 182px;">
<form action="/ProtectedView/Search/Find" data-ajax="true" data-ajax-failure="JavaScript:alert(&#39;ВНИМАНЕ: Возникла временная перегрузка сервера. Повторите попытку через некоторое время&#39;)" data-ajax-loading="#LoadingElem" data-ajax-mode="replace" data-ajax-success="SetSearchData(data)" data-ajax-update="#SearchRezultPanel" id="form1" method="post"><input id="Query" name="Query" style="width:100%" type="text" value="" />            <input type="button" onclick="TrySend()" value="Искать" style="width: 100%"></input>
            <input type="button" onclick="UnsetSearch()" value="Очистить поиск" style="width: 100%"></input>
</form>    </div>
    <div id="SearchRezultPanel" class="SearchRezultPanel">
        вывод результатов поиска
    </div>
    
    <script type="text/javascript">
        var SearchData;
        function TrySend() {
            if ($("#Query").val().length > 2)
            {
                var $form = $('#form1');
                $.ajax({
                    type: "POST",
                    url: $form.attr('action'),
                    data: $form.serialize(),
                    error: function (xhr, status, error) {
                        alert("При обработке формы запроса произошла ошибка!");
                    },
                    beforeSend:function()
                    {
                        $("#LoadingElem").show();
                    },
                    success: function (data) {
                        $("#LoadingElem").hide();
                        SetSearchData(data);
                    }
                });
            }
            else {
                alert("Минимальная длина поисковвой строки- 3 символа");
            }
        }
        function SetSearchData(data) {
            SearchData = data;
            RenderSearchPanel(data);
            SetPageHighlight();
        }
        function SetPageHighlight() {
            if (SearchData != null) {
                $("#HightlightContainer").empty();
                var k =  $("#slider2").slider("value") / 100;
                var j = -1;
                for (var i = 0; i < SearchData.DT.length; i++) {
                    if (SearchData.DT[i].PageN == $("#bmkpagenum").text()) {
                        //alert(i);
                        j = i;
                    }
                }
                if (j != -1)
                    for (var i = 0; i < SearchData.DT[j].PositionList.length; i++) {
                        //alert('!!');
                        $("#HightlightContainer").append("<div class='data2' style='top:" + SearchData.DT[j].PositionList[i].y * k * SearchData.DT[j].scalek + "px;left:" + SearchData.DT[j].PositionList[i].x * k * SearchData.DT[j].scalek + "px;height:" + SearchData.DT[j].PositionList[i].height * k * SearchData.DT[j].scalek + "px;width:" + SearchData.DT[j].PositionList[i].width * k * SearchData.DT[j].scalek + "px;'></div>");
                    }
            }
            //$("#HightlightContainer").append("<div class='data2' style='top:" + data[0].PositionList[1].y * k + "px;left:" + data[0].PositionList[1].x * k + "px;height:" + data[0].PositionList[1].height * k + "px;width:" + data[0].PositionList[1].width * k + "px;border:2px solid red'></div>")
            //$("#HightlightContainer").append("<div class='data2' style='top:" + data[0].PositionList[2].y * k + "px;left:" + data[0].PositionList[2].x * k + "px;height:" + data[0].PositionList[2].height * k + "px;width:" + data[0].PositionList[2].width * k + "px;border:2px solid red'></div>")
        }
        function UnsetSearch() {
            SearchData = null;
            $("#HightlightContainer").empty();
            $("#SearchRezultPanel").empty();
        }
        function RenderSearchPanel(data) {
            $("#SearchRezultPanel").html('');
            $("#SearchRezultPanel").append('<div>Найденно: ' + data.totalnum + ' совпадений</div>');
            for (var i = 0; i < data.DT.length; i++) {
                $("#SearchRezultPanel").append('' +
                        "<div class='SearchItem'><a onclick='GoToPage(" + data.DT[i].PageN + ")'><strong>[стр" + data.DT[i].PageN + "]</strong></a><br/>" + data.DT[i].ContextText +
                        "<div class='Sum'>Всего совпадений на странице " + data.DT[i].PositionList.length + "</div></div>");
            }
        }

        function GoToPage(page)
        {
            $.ajax({
                type: "GET",
                url: '/ProtectedView/Book/ViewBookPage/28181?page=' + page,
                //data: "{id:'" + 28181 + "',page:'"+page+"'}",
                contentType: "application/json; charset=utf-8",
                dataType: "html",//,
                beforeSend: function () {
                    //alert('!ВНИМАНИЕ');
                    GetScroll();
                    $("#LoadingElem").show();
                },
                success: function (data) {
                    $("#LoadingElem").hide();
                    //alert('!');
                    SetPageData(data);
                },
                error: function () {
                    alert('ВНИМАНИЕ: Возникла временная перегрузка сервера. Посчитайте до трех и повторите попытку.');
                }
            });
        }
    </script>
</div>

    </div>
</div>

